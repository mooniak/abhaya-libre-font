    1   # Latin-1 Supplement
    2   # Latin Extended-A
    73  # Sinhala
    ;
  TypoAscender 758;
  TypoDescender -266;
  TypoLineGap 204;
  winAscent 776;
  winDescent 246;
  Vendor "mnik";
} OS/2;


