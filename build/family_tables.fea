# -----------------------
# Horizontal Header Table
# -----------------------

table hhea {
  Ascender 758;
  Descender -266;
  LineGap 204;
} hhea;


# ------------------------------
# OS/2 and Windows Metrics Table
# ------------------------------

table OS/2 {
  FSType 0;
  Panose 2 0 5 0 0 0 0 0 0 0;
